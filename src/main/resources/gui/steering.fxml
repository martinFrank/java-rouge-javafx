<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="NEVER" minWidth="30.0" prefWidth="30.0"/>
        <ColumnConstraints hgrow="NEVER" minWidth="30.0" prefWidth="30.0"/>
        <ColumnConstraints hgrow="NEVER" minWidth="30.0" prefWidth="30.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="NEVER"/>
        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="NEVER"/>
    </rowConstraints>
    <children>
        <Button fx:id="steeringN" text="N" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Button fx:id="steeringS" text="S" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Button fx:id="steeringW" text="W" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Button fx:id="steeringE" text="E" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
    </children>
</GridPane>
